{
  "name": "@shepherdorg/cli",
  "version": "2.2.1",
  "description": "Command line interfaces for Shepherd",
  "keywords": [],
  "author": "Gu√∞laugur S. Egilsson <gulli@kolibri.is>",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShepherdOrg/npm-packages"
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "shepherd-build-docker": "bin/shepherd-build-docker.sh",
    "shepherd-deploy": "bin/shepherd-deploy.sh",
    "shepherd-delete-expired-resources": "bin/delete-expired-resources.sh",
    "kube-filter-expired-resources.js": "dist/kube-filter-expired-resources.js",
    "shepherd-inspect": "dist/shepherd-inspect-cmd.js",
    "join-metadata-files": "dist/join-metadata-files.js",
    "shepherd.js": "dist/shepherd.js"
  },
  "scripts": {
    "specs": "mocha --exit --require ts-node/register --recursive 'src/**/*.spec.ts'",
    "tdd": "mocha --exit -R dot --require ts-node/register --recursive --watch-extensions ts,js,sh --watch 'src/**/*.spec.ts'",
    "e2etest": "mocha --exit --require ts-node/register --recursive 'e2etest/**/*.e2e.ts'",
    "e2etdd": "mocha --exit -R dot --require ts-node/register --recursive --watch-extensions ts,js,sh --watch 'e2etest/**/*.e2e.ts'",
    "test": "yarn specs && yarn e2etest",
    "build": "tsc && chmod +x ./dist/shepherd-inspect-cmd.js",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@shepherdorg/deployer": "^2.2.1",
    "@shepherdorg/docker-image-metadata-loader": "^2.2.1",
    "@shepherdorg/metadata": "^2.2.0",
    "@shepherdorg/nano-inject": "^2.2.0",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "chai": "^4.2.0",
    "child-process-promise": "2.2.1",
    "fluture": "^11.0.3",
    "glob": "^7.1.6",
    "mocha": "^6.2.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
